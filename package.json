{
  "name": "desprendete",
  "version": "1.0.0",
  "description": "desprendete",
  "author": "Ezequiel Ger√≥nimo Gonzalez <ezequiel@adaitw.com.ar>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "pm2:start": "pm2 start npm --name $APP_NAME -- start",
    "pm2:restart": "pm2 stop $APP_NAME && pm2 delete $APP_NAME && npm run pm2:start",
    "pm2:stop": "pm2 stop $APP_NAME && pm2 delete $APP_NAME",
    "pm2": "if pm2 list | grep $APP_NAME; then npm run pm2:restart; else npm run pm2:start; fi"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-vue": "^1.0.2",
    "nuxt": "^1.0.0-rc11",
    "popper.js": "^1.12.9",
    "vue-masonry": "^0.10.12",
    "vue-no-ssr": "^0.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.6.1",
    "sass-loader": "^6.0.6"
  }
}
